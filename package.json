{
	"name": "advent-of-code",
	"packageManager": "yarn@4.0.2",
	"type": "module",
	"scripts": {
		"day1": "ts-node src/solutions/2023/01/part1.ts && ts-node src/solutions/2023/01/part2.ts",
		"day2": "ts-node src/solutions/2023/02/part1.ts && ts-node src/solutions/2023/02/part2.ts",
		"day3": "ts-node src/solutions/2023/03/part1.ts && ts-node src/solutions/2023/03/part2.ts",
		"day4": "ts-node src/solutions/2023/04/part1.ts && ts-node src/solutions/2023/04/part2.ts",
		"day5": "ts-node src/solutions/2023/05/part1.ts",
		"day6": "ts-node src/solutions/2023/06/part1.ts && ts-node src/solutions/2023/06/part2.ts",
		"day7": "ts-node src/solutions/2023/07/part1.ts && ts-node src/solutions/2023/07/part2.ts",
		"day8": "ts-node src/solutions/2023/08/part1.ts && ts-node src/solutions/2023/08/part2.ts",
		"day9": "ts-node src/solutions/2023/09/part1.ts && ts-node src/solutions/2023/09/part2.ts",
		"day10": "ts-node src/solutions/2023/10/part1.ts",
		"day11": "ts-node src/solutions/2023/11/part1.ts && ts-node src/solutions/2023/11/part2.ts",
		"format": "prettier --write src && eslint src --fix --format=pretty",
		"lint": "prettier --check src && eslint src --format=pretty",
