		"test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js"
	},
	"devDependencies": {
		"@types/jest": "^29.5.11",
		"@types/node": "^18.19.3",
		"eslint": "^8.55.0",
		"eslint-config-neon": "^0.1.57",
		"eslint-formatter-pretty": "^5.0.0",
		"husky": "^8.0.3",
		"jest": "^29.7.0",
		"lint-staged": "^15.2.0",
		"prettier": "^3.1.1",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	}
}
